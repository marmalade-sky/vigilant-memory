  "bugs": {
    "url": "https://github.com/marmalade-sky/vigilant-memory/issues"
  },
  "homepage": "https://github.com/marmalade-sky/vigilant-memory#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {}
